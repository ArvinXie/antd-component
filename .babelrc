{
    "presets": [
        ["@babel/preset-env", { "modules": false }],// 保留es6模块引入的方式
        "@babel/react"
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        [
            "import",
            {
                "libraryName": "antd",
                "libraryDirectory": "lib",
                "style": "less"
            }
        ]
    ],
    "comments": false
}